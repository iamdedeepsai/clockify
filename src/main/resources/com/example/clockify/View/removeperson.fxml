<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="bg" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clockify.Controller.RemoveController">
   <children>
      <Button fx:id="remove" layoutX="334.0" layoutY="141.0" mnemonicParsing="false" onAction="#removeperson" prefHeight="200.0" prefWidth="200.0" text="Remove">
         <font>
            <Font name="SFBold" />
         </font>
      </Button>
      <Label fx:id="name" alignment="CENTER" contentDisplay="CENTER" layoutX="334.0" layoutY="64.0" prefHeight="39.0" prefWidth="200.0" style="-fx-border-width: 0.3em;" text="Do you want to remove Sai?">

      </Label>
      <TextField fx:id="search" layoutX="70.0" layoutY="71.0" prefHeight="25.0" prefWidth="200.0"  styleClass="nofocus" />
      <TableView fx:id="table" layoutX="70.0" layoutY="141.0" prefHeight="200.0" prefWidth="200.0" styleClass="nofocus" >
        <columns>
          <TableColumn fx:id="names" prefWidth="199.0" text="Names" styleClass="nofocus" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
